.container-fluid.smoke
	.row
		.col-xs-8.col-xs-offset-2.content
			.page-header.text-center
				%h1 Your Songs
			.well.songs
				%p Hello
				.col-xs-6.col-xs-offset-3
					= form_for(@musician, url: song_upload_musician_path(current_user.id), method: :post) do |f|
						.form-group
							= f.file_field "song_file", class: "form-control avatar"
						= f.submit "Upload Song", class: "btn btn-info btn-block"